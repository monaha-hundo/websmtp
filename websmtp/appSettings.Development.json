{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Database": {
    "Server": "localhost",
    "Name": "websmtp",
    "Username": "websmtp",
    "Password": "websmtp"
  },
  "Security": {
    "EnableHtmlDisplay": true,
    "EnableMediaInHtml": true
  },
  "DKIM": {
    "SigningEnabled": false,
    "Domains": [
      {
        "Name": "websmtp.local",
        "Selector": "dkim",
        "PrivateKey": "dkim_private.dev.pem"
      }
    ]
  },
  "SPF": {
    "Enabled": true
  },
  "DNS": {
    "IP": "192.168.1.1",
    "Port": 53
  },
  "SpamAssassin": {
    "Enabled": true
  },
  "SMTP": {
    "Port": 1025,
    "RemotePort": 1025
  },
  "AllowedHosts": "*"
}